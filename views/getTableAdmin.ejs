<!DOCTYPE html>
<html>
<head>
  <title>Panel de Administración</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
</head>
<body>
  <div class="container">
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav">
            <li class="nav-item">
              <a class="nav-link" href="/html/estudiantes">Estudiantes</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/html/asignaturas">Asignaturas</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/html/grupos">Grupos</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/html/aulas">Aulas</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/html/horarios">Horarios</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/html/matriculas">Matrículas</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/html/profesores">Profesores</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/html/programas">Programas</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/html/panel">Panel</a>
            </li>
          </ul>
        </div>
      </nav>
    <h1 class="mt-4">Panel de Administración</h1>
    <table class="table mt-4">
      <thead class="thead-dark">
        <tr>
          <th>ID Estudiante</th>
          <th>Nombre Estudiante</th>
          <th>Apellido Estudiante</th>
          <th>Estrato</th>
          <th>Género Estudiante</th>
          <th>Ciudad de Nacimiento</th>
          <th>Fecha de Nacimiento</th>
          <th>Dirección</th>
          <th>ID Matrícula</th>
          <th>ID Asignatura</th>
          <th>ID Profesor</th>
          <th>ID Programa</th>
          <th>Nombre Asignatura</th>
          <th>Creditos</th>
          <th>ID Aula</th>
          <th>Día</th>
          <th>Hora de Inicio</th>
          <th>Hora de Fin</th>
          <th>Nombre Aula</th>
          <th>Ubicación Aula</th>
          <th>Capacidad Aula</th>
          <th>Tipo Aula</th>
          <th>Nombre Programa</th>
          <th>Facultad Programa</th>
          <th>ID Grupo</th>
          <th>Número de Estudiantes</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach((row) => { %>
          <tr>
            <td><%= row.id_estudiante %></td>
            <td><%= row.nom_estudiante %></td>
            <td><%= row.ape_estudiante %></td>
            <td><%= row.estrato %></td>
            <td><%= row.genero_estudiante %></td>
            <td><%= row.ciudad_nac %></td>
            <td><%= row.fecha_nac %></td>
            <td><%= row.direccion %></td>
            <td><%= row.id_matricula %></td>
            <td><%= row.id_asignatura %></td>
            <td><%= row.id_profesor %></td>
            <td><%= row.id_programa %></td>
            <td><%= row.nom_asignatura %></td>
            <td><%= row.creditos %></td>
            <td><%= row.id_aula %></td>
            <td><%= row.dia %></td>
            <td><%= row.hora_inicio %></td>
            <td><%= row.hora_fin %></td>
            <td><%= row.nom_aula %></td>
            <td><%= row.ubicacion %></td>
            <td><%= row.capacidad %></td>
            <td><%= row.tipo %></td>
            <td><%= row.programa %></td>
            <td><%= row.facultad %></td>
            <td><%= row.id_grupo %></td>
            <td><%= row.num_estudiantes %></td>
            <!-- Agrega más columnas aquí según tus datos -->
          </tr>
        <% }); %>
      </tbody>
    </table>
  </div>

  <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.3/dist/umd/popper.min.js"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
</body>
</html>
